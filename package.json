{
    "name": "marvel-for-noobs",
    "version": "0.3.0",
    "private": true,
    "dependencies": {
        "ansi-html": "^0.0.9",
        "axios": "^0.27.2",
        "classnames": "^2.3.1",
        "logrocket": "^3.0.0",
        "prop-types": "^15.8.1",
        "react": "^18.1.0",
        "react-countdown": "^2.3.2",
        "react-dom": "^18.1.0",
        "react-ga": "^3.3.0",
        "react-router": "^6.3.0",
        "react-router-dom": "^6.3.0",
        "universal-cookie": "^4.0.4"
    },
    "scripts": {
        "build-css": "node-sass --output-style expanded --include-path ./src --include-path ./node_modlues src/ -o src/",
        "watch-css": "npm run build-css && node-sass --output-style expanded --include ./src --include-path: ./node_modules src/ -o src/ --watch --recursive",
        "start": "webpack serve --config webpack.config.dev.js --hot",
        "build": "webpack --config webpack.config.prod.js"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 11",
        "not op_mini all"
    ],
    "devDependencies": {
        "@babel/core": "^7.18.2",
        "@babel/preset-env": "^7.18.2",
        "@babel/preset-react": "^7.17.12",
        "babel-loader": "^8.2.5",
        "css-loader": "^6.7.1",
        "html-webpack-plugin": "^5.5.0",
        "node-sass": "^7.0.1",
        "style-loader": "^3.3.1",
        "webpack": "^5.73.0",
